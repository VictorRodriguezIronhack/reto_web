                    <div className=\"text-center mt-4\">\n                        <Button onClick={() => this.handleModal(true)} variant=\"dark\" size=\"lg\" className=\"mb-5 mt-4\">Add New Phone</Button>\n                    </div>\n\n                </Container>            \n\n                 <Popup show={this.state.showModal} handleModal={this.handleModal}>\n                    <PhoneForm closeModal={() => this.handleModal(false)} updateList={this.displayPhones} {...this.props}/>\n                </Popup>             \n            </> \n        )\n    }\n} \n\nexport default PhoneList","/Users/SaraMartz/Desktop/reto_web/client/src/components/shared/Popup/Popup.js",[],"/Users/SaraMartz/Desktop/reto_web/client/src/components/pages/Phones/List/Phone-card.js",[],"/Users/SaraMartz/Desktop/reto_web/client/src/service/phones.service.js",[],"/Users/SaraMartz/Desktop/reto_web/client/src/components/pages/Phones/Details/Phone-details.js",["67"],"import React, { Component } from 'react'\nimport { Container, Row, Col, Button } from 'react-bootstrap'\nimport PhoneService from '../../../../service/phones.service'\nimport PhoneEdit from \"../Edit/Phone-edit\"\nimport Popup from \"../../../shared/Popup/Popup\"\n\nclass PhoneDetails extends Component {\n\n    constructor(props) {\n        super(props)\n        this.state = {\n            phone: {},       \n            showModal: false,\n           \n        }\n        this.phonesService = new PhoneService()\n    }\n\n    componentDidMount = () => this.displayPhone() \n\n    displayPhone = () => {\n        const id = this.props.match.params.id\n\n        this.phonesService\n            .getPhone(id)\n            .then(res => this.setState({ phone: res.data }))        \n            .catch(err => console.log(err))   \n    }\n\n    deletePhone = () => {\n        const id = this.props.match.params.id\n\n        this.phonesService\n            .deletePhone(id)\n            .then(res => {\n                this.setState({ phone: res.data })\n                this.props.history.push(`/`)  \n            })\n            .catch(err => console.log(err))   \n    }\n\n    handleModal = visible => this.setState({ showModal: visible })\n\n    render() {\n\n        return (        \n      \n            <Container className=\"phone-container mt-5\">               \n           \n                <Row>\n                    <Col md={5} className=\"text-center\">\n                        {this.state.phone.name && <h2>{this.state.phone.name.toUpperCase()}</h2> }\n                    </Col>\n                </Row>\n                    <hr></hr>                \n                <Row>\n                    <Col md={5} className=\"d-flex flex-column align-items-center text-center\">\n                \n                        <img src={this.state.phone.imageFileName} className=\"mb-4 mt-4 phone-list-img\"></img>             \n               \n\n                        <div>\n                            <Button onClick={() => this.handleModal(true)} variant=\"light grey\" size=\"sm\" className=\"text-center\">Edit</Button>                    \n                            <Button onClick={() => this.deletePhone()} variant=\"black\" size=\"sm\" className=\"\">Delete</Button>\n                        </div>\n                    </Col>\n                                \n                    <Col md={6} className=\"phone-details text-justify mt-4\">\n                        {this.state.phone.color && <p className=\"mt-1\"><strong>Color:</strong>{this.state.phone.color.trim().replace(/^\\w/, (c) => c.toUpperCase())}</p>}\n                        <p><strong>Price:</strong>{this.state.phone.price}€</p>\n                        <p><strong>Ram:</strong>{this.state.phone.ram}GB</p>\n                        <p><strong>Screen:</strong>{this.state.phone.screen}</p>\n                        <p><strong>Processor:</strong>{this.state.phone.processor}</p>\n                        <p><strong>Manufacturer:</strong>{this.state.phone.manufacturer}</p>\n                        <p><strong>Description:</strong>{this.state.phone.description}</p>        \n                    </Col>\n                </Row> \n\n                <Popup show={this.state.showModal} handleModal={this.handleModal}>\n                    <PhoneEdit closeModal={() => this.handleModal(false)} updatePhone={this.displayPhone} {...this.props}/>\n                </Popup>\n        \n            </Container> \n    \n        )    \n    }\n}\n\nexport default PhoneDetails","/Users/SaraMartz/Desktop/reto_web/client/src/components/pages/Phones/Edit/Phone-edit.js",[],"/Users/SaraMartz/Desktop/reto_web/client/src/service/upload.service.js",[],"/Users/SaraMartz/Desktop/reto_web/client/src/components/shared/Spinner/Spinner.js",["68"],"import './Spinner.css'\nexport default () => <div className=\"loader\"></div>",["69","70"],"/Users/SaraMartz/Desktop/reto_web/client/src/components/pages/Phones/New/Phone-form.js",[],"/Users/SaraMartz/Desktop/reto_web/client/src/components/layout/Footer/Footer.js",["71","72","73","74","75","76"],"import { Row, Col, Container } from 'react-bootstrap'\nimport { MDBFooter } from \"mdbreact\";\nimport './Footer.css'\n\nimport codepen from './codepen.png'\nimport linkedin from './linkedin.png'\nimport github from './github.png'\n\nconst Footer = () => {\n    return (\n        <MDBFooter className=\"text-center dark grey footer\"> \n            <Container>\n                <Row className=\"d-flex justify-content-center\">\n                    <Col xs={12} sm={6} md={4} >\n                        <ul className=\"social-icons mt-3 mb-3\">             \n                            <li><a className=\"github\" target=\"_blank\" href=\"https://github.com/saramartz\"><img src={github} style={{width: 20, height: 20, opacity: 0.8}} className=\"footer-icon mb-1\"></img></a></li>   \n                            <li><a className=\"linkedin\" target=\"_blank\" href=\"https://www.linkedin.com/in/sara-mart%C3%ADnez-vega-5a25991b9/\"><img src={