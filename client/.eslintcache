          <Form.Control type=\"text\" name=\"description\" value={this.state.description} onChange={this.handleInputChange} />\n                                </Form.Group>\n                                <Form.Group controlId=\"color\">\n                                    <Form.Label>Color</Form.Label>\n                                    <Form.Control type=\"text\" name=\"color\" value={this.state.color} onChange={this.handleInputChange} />\n                                </Form.Group>\n                                <Form.Group controlId=\"price\">\n                                    <Form.Label>Precio</Form.Label>\n                                    <Form.Control type=\"text\" name=\"price\" value={this.state.precio} onChange={this.handleInputChange} />\n                                </Form.Group>\n                                <Form.Group controlId=\"image\">\n                                    <Form.Label>Imagen (archivo)</Form.Label>\n                                    <Form.Control type=\"file\" onChange={this.handleImageUpload} />\n                                </Form.Group>\n                                <Form.Group controlId=\"sreen\">\n                                    <Form.Label>Tama침o de la pantalla</Form.Label>\n                                    <Form.Control type=\"text\" name=\"screen\" value={this.state.screen} onChange={this.handleInputChange} />\n                                </Form.Group>\n                                <Form.Group controlId=\"processor\">\n                                    <Form.Label>Procesador</Form.Label>\n                                    <Form.Control type=\"text\" name=\"processor\" value={this.state.processor} onChange={this.handleInputChange} />\n                                </Form.Group>\n                                <Form.Group controlId=\"ram\">\n                                    <Form.Label>RAM</Form.Label>\n                                    <Form.Control type=\"text\" name=\"ram\" value={this.state.ram} onChange={this.handleInputChange} />\n                                </Form.Group>\n                                <br>\n\n                                </br>\n                                <Button variant=\"dark\" type=\"submit\" >Crear m칩vil</Button>\n\n                            </Form>\n                        </Col>\n                    </Row>\n                </Container>\n            </>\n        )\n    }\n}\n\nexport default PhoneForm\n","/home/alicia/directorio ironhack/prueba/client/src/Service/Phone.service.js",[],"/home/alicia/directorio ironhack/prueba/client/src/components/Pages/Phone/PhoneList.js",[],"/home/alicia/directorio ironhack/prueba/client/src/components/Pages/Phone/PhoneCard.js",["73"],"/home/alicia/directorio ironhack/prueba/client/src/components/Pages/Phone/PhoneDetails.js",["74","75","76","77","78"],"/home/alicia/directorio ironhack/prueba/client/src/Service/Upload.service.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"79","replacedBy":"83"},{"ruleId":"81","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":2,"column":25,"nodeType":"87","messageId":"88","endLine":2,"endColumn":33},{"ruleId":"85","severity":1,"message":"86","line":4,"column":10,"nodeType":"87","messageId":"88","endLine":4,"endColumn":18},{"ruleId":"89","severity":1,"message":"90","line":12,"column":21,"nodeType":"91","endLine":12,"endColumn":54},{"ruleId":"85","severity":1,"message":"92","line":3,"column":10,"nodeType":"87","messageId":"88","endLine":3,"endColumn":14},{"ruleId":"85","severity":1,"message":"93","line":4,"column":21,"nodeType":"87","messageId":"88","endLine":4,"endColumn":24},{"ruleId":"85","severity":1,"message":"94","line":4,"column":26,"nodeType":"87","messageId":"88","endLine":4,"endColumn":29},{"ruleId":"85","severity":1,"message":"95","line":4,"column":31,"nodeType":"87","messageId":"88","endLine":4,"endColumn":37},{"ruleId":"89","severity":1,"message":"90","line"